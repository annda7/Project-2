        <form class="card-header">
          <div class="form-group">
            <label for="description">Description</label>
            <input type="description" class="form-control" id="description" placeholder="What do you have to do?">
          </div>
          <div class="form-check">
            <input class="form-check-input" type="radio" name="inOutRadio" id="indoor" value="indoor">
            <label class="form-check-label" for="indoor">
              Indoor
            </label>
          </div>
          <div class="form-check">
            <input class="form-check-input" type="radio" name="inOutRadio" id="outdoor" value="outdoor">
            <label class="form-check-label" for="outdoor">
              Outdoor
            </label>
          </div>
          <button type="submit" class="btn btn-primary">Submit</button>
        </form>

        </div>

<!-- ////Middle part/// -->
      <div class="col-md-4">
        <p class="subheader">Welcome{{ user.first}}, Please check the weather and choose a task below!</p>
      </div>
<!-- ///end middle part/// -->


      <div class="col-md-4">
        <div class="card-header">
            <span class="currently">
                <span class="clear-night-icon-currently currently-icon swip">
